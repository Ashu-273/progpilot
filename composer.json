{
    "name": "designsecurity/progpilot",
    "description": "A Static Analyser for security",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Therond",
            "email": "eric.therond.fr@gmail.com"
        }
    ],
    "require": {
        "cweagans/composer-patches": "~1.0",
        "ircmaxell/php-cfg": "*",
        "symfony/yaml": "~3.3.6"
    },
    "extra": {
		"enable-patching": true,
		"patches": {
			"ircmaxell/php-cfg": {
				"Op": "https://raw.githubusercontent.com/designsecurity/progpilot/master/patchs/Op.diff",
				"Include": "https://raw.githubusercontent.com/designsecurity/progpilot/master/patchs/Include_.diff",
				"Traverser": "https://raw.githubusercontent.com/designsecurity/progpilot/master/patchs/Traverser.diff"
            }
        }
    },
    "bin": ["progpilot"],
    "autoload": {
        "psr-0": {
            "progpilot": "package/src"
        }
    }
}
