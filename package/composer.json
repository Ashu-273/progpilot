{    
    "name": "progpilot/package",
    "description": "A Static Analyser for security (package for local dev)",
    "license": "MIT",
    "authors": [
        {
            "name": "Eric Therond",
            "email": "eric.therond.fr@gmail.com"
        }
    ],
    "require": {
        "cweagans/composer-patches": "~1.0",
        "ircmaxell/php-cfg": "dev-master",
        "symfony/yaml": "~3.3.6"
    },
	"extra": {
		"patches": {
			"ircmaxell/php-cfg": {
				"Op": "../../patchs/Op.diff",
				"Include": "../../patchs/Include_.diff",
				"Traverser": "../../patchs/Traverser.diff"
				}
			}
		},
    "autoload": {
        "psr-0": {
            "progpilot": "src"
        }
    }
} 
